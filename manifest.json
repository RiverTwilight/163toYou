{
	"name": "163ToYou",
	"description": "Build an Extension!",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["contextMenus"],
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["index.js"],
			"run_at": "document_start"
		}
	]
}
